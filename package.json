{
  "private": true,
  "description": "Website, demo, guides and documentation of the list-entities-vue package",
  "workspaces": ["pkg"],
  "scripts": {
    "start": "snowpack dev --config ./snowpack.web.config.mjs",
    "build:web": "snowpack build --config ./snowpack.web.config.mjs",
    "build:npm": "snowpack build --config ./snowpack.npm.config.mjs",
    "build": "snowpack build --config ./snowpack.web.config.mjs && snowpack build --config ./snowpack.npm.config.mjs",
    "test": "jest",
    "lint": "eslint src --fix"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@snowpack/plugin-babel": "^2.1.7",
    "@snowpack/plugin-vue": "^2.6.2",
    "@vue/test-utils": "^2.1.0",
    "@vue/vue3-jest": "^29.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.1.0",
    "eslint": "^8.24.0",
    "eslint-plugin-vue": "^9.5.1",
    "jest": "^29.1.1",
    "jest-environment-jsdom": "^29.1.1",
    "jsdom": "^20.0.0",
    "snowpack": "^3.3.7",
    "snowpack-plugin-less": "^1.0.7"
  },
  "dependencies": {
    "vue": "^3.2.40"
  }
}
